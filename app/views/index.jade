extends layouts/layout

block content
  h1= title
  p Welcome to #{title}
  input(id="deleteUser" data-user-id=userId type="button" value="Delete")
  input(id="logOut" type="button" value="Log Out")
  form(action="/upload" method="post" enctype='multipart/form-data')
    input(id="uploadModel" type="file" name="model")
    input(type="submit" value="Upload")
  script(src='/javascripts/jquery-1.10.2.js')
  script(src='/javascripts/index.js')